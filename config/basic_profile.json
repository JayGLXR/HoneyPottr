{
  "profile": {
    "name": "basic",
    "description": "Basic honeypot profile with minimal footprint"
  },
  "artifacts": {
    "modules": {
      "registry": {
        "enabled": true,
        "intensity": "low",
        "refreshInterval": 7200,
        "parameters": {
          "createVMwareKeys": true,
          "createVirtualBoxKeys": false,
          "createSandboxieKeys": false,
          "createAnalysisToolKeys": false
        }
      },
      "filesystem": {
        "enabled": true,
        "intensity": "medium",
        "refreshInterval": 14400,
        "parameters": {
          "createVMwareFiles": true,
          "createVirtualBoxFiles": false,
          "createAnalysisToolFiles": false,
          "createDecoyFiles": false,
          "filePermissions": "readonly"
        }
      },
      "processes": {
        "enabled": false,
        "intensity": "low",
        "refreshInterval": 3600,
        "parameters": {
          "simulateVMwareServices": false,
          "simulateVirtualBoxServices": false,
          "simulateAnalysisTools": false
        }
      },
      "network": {
        "enabled": false,
        "intensity": "low",
        "refreshInterval": 7200,
        "parameters": {
          "createVirtualAdapters": false,
          "simulateVMwareNetwork": false,
          "simulateVirtualBoxNetwork": false
        }
      },
      "hardware": {
        "enabled": true,
        "intensity": "low",
        "refreshInterval": 86400,
        "parameters": {
          "simulateVMwareBIOS": true,
          "simulateVirtualBoxBIOS": false,
          "simulateVirtualHardware": false
        }
      }
    }
  }
}
