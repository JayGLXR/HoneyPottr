{
  "profile": {
    "name": "advanced",
    "description": "Advanced honeypot profile with comprehensive coverage"
  },
  "artifacts": {
    "modules": {
      "registry": {
        "enabled": true,
        "intensity": "high",
        "refreshInterval": 1800,
        "parameters": {
          "createVMwareKeys": true,
          "createVirtualBoxKeys": true,
          "createSandboxieKeys": true,
          "createAnalysisToolKeys": true
        }
      },
      "filesystem": {
        "enabled": true,
        "intensity": "maximum",
        "refreshInterval": 3600,
        "parameters": {
          "createVMwareFiles": true,
          "createVirtualBoxFiles": true,
          "createAnalysisToolFiles": true,
          "createDecoyFiles": true,
          "filePermissions": "readonly"
        }
      },
      "processes": {
        "enabled": true,
        "intensity": "medium",
        "refreshInterval": 900,
        "parameters": {
          "simulateVMwareServices": true,
          "simulateVirtualBoxServices": true,
          "simulateAnalysisTools": true,
          "processVisibility": "full"
        }
      },
      "network": {
        "enabled": true,
        "intensity": "medium",
        "refreshInterval": 1800,
        "parameters": {
          "createVirtualAdapters": false,
          "simulateVMwareNetwork": true,
          "simulateVirtualBoxNetwork": true
        }
      },
      "hardware": {
        "enabled": true,
        "intensity": "high",
        "refreshInterval": 43200,
        "parameters": {
          "simulateVMwareBIOS": true,
          "simulateVirtualBoxBIOS": true,
          "simulateVirtualHardware": true,
          "hardwareVisibility": "wmi_all"
        }
      }
    }
  },
  "service": {
    "updateInterval": 1800,
    "monitoringInterval": 15,
    "resourceLimits": {
      "maxMemoryMB": 100,
      "maxCpuPercent": 2.0,
      "maxDiskIOPS": 200,
      "maxNetworkBPS": 2048
    }
  }
}
